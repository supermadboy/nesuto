{
  "kind": "collectionType",
  "collectionName": "apartments",
  "info": {
    "singularName": "apartment",
    "pluralName": "apartments",
    "displayName": "Apartment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subTitle": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "numberOfRooms": {
      "required": true,
      "type": "integer"
    },
    "pictures": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "hashtags": {
      "type": "json",
      "required": false
    },
    "paymentType": {
      "type": "enumeration",
      "enum": [
        "RENT",
        "BUY"
      ]
    }
  }
}
